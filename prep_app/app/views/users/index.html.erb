<h1>Users#index</h1>

<% @users.each do |user| %>
  <ul>
    <li><%=link_to image_tag user.image, user %></li>
    <li><%= user.first_name %></li>
    <li><%= user.second_name %></li>
    <li><%= user.role %></li>
    <li><%= link_to 'Edit', edit_post_path(post) %></li>
  </ul>
<% end %>

<%= link_to 'New User', new_user_path %>
